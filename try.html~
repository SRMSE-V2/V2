<script>
window.URL = window.URL || window.webkitURL;  // Take care of vendor prefixes.

var xhr = new XMLHttpRequest();
xhr.open('GET', 'http://pixabay.com/static/uploads/photo/2014/04/07/06/20/business-cards-318418_640.jpg', true);
xhr.responseType = 'blob';
xhr.setRequestHeader ("Access-Control-Allow-Origin", "http://srmsearchengine.in");
xhr.setRequestHeader ("user-agent", "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/43.0.2357.132 Safari/537.36");
xhr.onload = function(e) {
  if (this.status == 200) {
    var blob = this.response;

    var img = document.createElement('img');
    img.onload = function(e) {
      window.URL.revokeObjectURL(img.src); // Clean up after yourself.
    };
    img.src = window.URL.createObjectURL(blob);
    document.body.appendChild(img);
  }
};

xhr.send();

</script>
<body>
</body>
